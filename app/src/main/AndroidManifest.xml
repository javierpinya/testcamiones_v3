<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.javierpinya.testcamiones_v3">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/avatar"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.javierpinya.testcamiones_v3.ResultadoBuscarVehiculoActivity"></activity>
        <activity android:name="com.javierpinya.testcamiones_v3.ResultadoInspeccionActivity" />
        <activity android:name="com.javierpinya.testcamiones_v3.BuscarInspeccionActivity" />
        <activity android:name="com.javierpinya.testcamiones_v3.BuscarVehiculoActivity" />
        <activity
            android:name="com.javierpinya.testcamiones_v3.MenuActivity"
            android:label="@string/title_activity_menu" />
        <activity android:name=".MainActivity" />
        <activity android:name="com.javierpinya.testcamiones_v3.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>